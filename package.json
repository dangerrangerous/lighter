{
  "name": "lighter",
  "description": "A lightweight Node.js framework",
  "keywords": [
    "lighter",
    "isomorphic",
    "mvc",
    "http",
    "https",
    "router",
    "express",
    "alternative"
  ],
  "version": "0.1.67",
  "main": "lighter.js",
  "bin": {
    "lighter": "commands/cli.js"
  },
  "homepage": "http://lighter.io/lighter",
  "repository": "git://github.com/lighterio/lighter.git",
  "bugs": {
    "url": "http://github.com/lighterio/lighter/issues"
  },
  "author": "Sam Eubank <sameubank@gmail.com>",
  "contributors": [
    "Sam Eubank <sameubank@gmail.com>"
  ],
  "engines": [
    "node >= 0.2.6"
  ],
  "scripts": {
    "test": "./node_modules/exam/exam.js",
    "retest": "./node_modules/exam/exam.js --watch",
    "cover": "istanbul cover ./node_modules/exam/exam.js",
    "report": "open coverage/lcov-report/index.html",
    "coveralls": "istanbul cover ./node_modules/exam/exam.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "dependencies": {
    "beams": "^0.0.16",
    "cedar": "^0.0.17",
    "chug": "^0.2.0",
    "d6": "^0.1.7",
    "jymin": "^0.3.5",
    "ltl": "^0.1.14",
    "mc": "^1.0.6",
    "ormy": "^0.1.4",
    "shellify": "^0.0.4",
    "splode": "^0.0.2",
    "za": "^0.0.21"
  },
  "devDependencies": {
    "exam": "^0.0.7",
    "istanbul": "0.3.2",
    "coveralls": "2.11.1",
    "express": "4.9.0",
    "zeriousify": "^0.1.9"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/lighterio/lighter/blob/master/MIT-LICENSE.md"
    }
  ]
}
